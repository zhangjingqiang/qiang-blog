<h1 class="page-header">Logins</h1>

<table class="table table-bordered table-striped table-hover table-condensed">
  <thead>
    <tr>
      <th>IP</th>
      <th>Created At</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <% @logins.each do |login| %>
      <tr>
        <td><%= login.ip %></td>
        <td><%=l login.created_at %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= will_paginate @logins, renderer: BootstrapPagination::Rails %>